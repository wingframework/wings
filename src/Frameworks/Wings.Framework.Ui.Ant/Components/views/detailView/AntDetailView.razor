@namespace Wings.Framework.Ui.Ant.Components
@inherits DetailViewBase<TModel>
@typeparam TModel

<Row Gutter="16" Style="margin-top:20px;">
    @foreach(var prop in PropertyList) { 
    <AntDesign.Col Class="gutter-row" Span="12">
        <AntDetailProp TModel="TModel" Value="Value" Property="prop"></AntDetailProp>
    </AntDesign.Col>
    }
  </Row>
    @code {
        int groups;
        protected override void OnInitialized()
        {
            base.OnInitialized();
            groups = int.Parse(Math.Ceiling(PropertyList.Count / 2.0).ToString());
            StateHasChanged();
        }
    }
